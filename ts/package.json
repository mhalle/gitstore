{
  "name": "@mhalle/vost",
  "version": "0.9.7",
  "description": "A versioned filesystem backed by a bare git repository",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:deno": "deno test tests/deno_compat_test.ts --allow-read --allow-write --allow-env --no-check",
    "prepack": "cp ../LICENSE . && npm run build",
    "postpack": "rm -f LICENSE"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhalle/vost.git",
    "directory": "ts"
  },
  "homepage": "https://github.com/mhalle/vost/tree/master/ts",
  "bugs": {
    "url": "https://github.com/mhalle/vost/issues"
  },
  "keywords": [
    "git",
    "filesystem",
    "versioned",
    "storage",
    "isomorphic-git"
  ],
  "dependencies": {
    "isomorphic-git": "^1.27.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "@types/node": "^22.0.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0"
}
